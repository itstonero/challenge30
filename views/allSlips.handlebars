<table class="table table-dark table-striped">
    <thead>
        <th>Total Amount</th>
        <th>Bonus Amount</th>
        <th>Trial</th>
        <th>Target</th>
    </thead>
    <tbody>
        {{#each data}}
            <tr onclick="openSlip({{this.id}})" style="cursor: pointer;">
                <td>{{  this.totalAmount }}</td>
                <td>{{  this.bonusAmount }}</td>
                <td>{{  this.retries }}</td>
                <td>{{  this.progress }}</td>
            </tr>
        {{/each}}
    </tbody>
</table>